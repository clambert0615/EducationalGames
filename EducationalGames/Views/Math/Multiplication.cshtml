<h3> Multiplication Problem</h3>
<br />
<h5>&nbsp;&nbsp; @ViewBag.num1 <br />
    *&nbsp;@ViewBag.num2
</h5>
<br />
<form action="/Math/MultAnswer" method="post">
    Answer:  <input type="number" id="answer" name="answer" />
    <input type="hidden" id="num1" name="num1" value="@ViewBag.num1" />
    <input type="hidden" id="num2" name="num2" value="@ViewBag.num2" />
    <input type="hidden" id="level" name="level" value="@ViewBag.Level" />
    <input type="submit" value="Submit Answer" />
</form>